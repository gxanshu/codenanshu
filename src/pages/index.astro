---
// Component Imports
import Default from "layouts/Default.astro";
import ProjectsCard from "components/cards/Projects.astro";
import Projects from "../projects.json";
import BlogCard from "components/cards/Blogs.astro";
import NotesCard from "components/cards/Notes.astro";

let blogs = await Astro.glob("./blog/*.mdx");
blogs = blogs.sort(
  (a, b) => Date.parse(b.frontmatter.date) - Date.parse(a.frontmatter.date)
);

let notes = await Astro.glob("./notes/*.mdx");
notes = notes.sort(
  (a, b) => Date.parse(b.frontmatter.date) - Date.parse(a.frontmatter.date)
);
// Full Astro Component Syntax:
// https://docs.astro.build/Default-concepts/astro-components/
---

<Default
  title="CodeNanshu - developer's coding blog"
  description="codenanshu is the coding & programming blog that write about web development, artifical intelligence and machine learning. help you to understand computers"
>
  <!-- hero section -->
  <section class="text-gray-500 body-font">
    <div
      class="container mx-auto flex px-5 py-24 md:flex-row flex-col items-center"
    >
      <div
        class="lg:flex-grow md:w-1/2 lg:pr-24 md:pr-16 flex flex-col md:items-start md:text-left mb-16 md:mb-0 items-center text-center"
      >
        <h1
          class="font-heading sm:text-4xl text-3xl mb-4 font-medium text-black"
        >
          I am <span
            class="text-transparent bg-clip-text bg-gradient-to-r from-blue-600 to-indigo-600"
            >Anshu</span
          >
          <br class="hidden lg:inline-block" />trying to building a <span
            class="text-transparent bg-clip-text bg-gradient-to-r from-blue-600 to-indigo-600"
            >new web.</span
          >
        </h1>
        <p class="mb-8 leading-relaxed">
          I specialize in rapidly building software companies and web
          applications. I talk about my journey on Twitter, commit code to
          Github, and take shots on Dribbble. you can find me in your computer.
        </p>
        <div class="flex justify-center">
          <a href="/about"
            ><button
              class="inline-flex text-white bg-blue-600 border-0 py-2 px-6 focus:outline-none hover:bg-indigo-600 rounded text-lg"
              >About</button
            >
          </a>
          <a href="/contact"
            ><button
              class="ml-4 inline-flex text-gray-300 bg-gray-900 border-0 py-2 px-6 focus:outline-none hover:bg-gray-700 hover:text-white rounded text-lg"
              >Contact</button
            >
          </a>
        </div>
      </div>
      <div class="lg:max-w-lg lg:w-full md:w-1/2 w-5/6">
        <img class="animation-20s" alt="hero" src="/programming.jpg" />
      </div>
    </div>
  </section>

  <!-- projects section -->
  <section class="mx-auto max-w-screen-xl items-center gap-8 mt-10">
    <div class="flex justify-between">
      <h3
        class="font-heading text-md sm:text-xl mb-3 font-extrabold text-transparent bg-clip-text bg-gradient-to-r from-indigo-500 to-green-500"
      >
        Some of My Projects
      </h3>
      <a
        class="font-heading text-gray-700"
        href="https://github.com/aianshume?tab=repositories"
        target="_blank">View All</a
      >
    </div>
    <div class="text-gray-500 body-font">
      <div class="container px-5 py-12 mx-auto">
        <div class="flex flex-wrap -mx-4 -my-8">
          {
            Projects.data.map((project) => {
              return <ProjectsCard data={project} />;
            })
          }
        </div>
      </div>
    </div>
  </section>

  <!-- Blog section -->
  <section class="mx-auto max-w-screen-xl items-center gap-8 mt-10">
    <div class="flex justify-between">
      <h3
        class="font-heading text-md sm:text-xl mb-3 font-extrabold text-transparent bg-clip-text bg-gradient-to-r from-indigo-500 to-green-500"
      >
        Latest Articles
      </h3>
      <a class="font-heading text-gray-700" href="/blog">View All</a>
    </div>
    <div class="relative items-center w-full py-12 mx-auto">
      <div
        class="grid w-full grid-cols-1 gap-6 mx-auto md:grid-cols-2 lg:grid-cols-4"
      >
        {
          [0, 1, 2, 3].map((index) => (
            <BlogCard
              frontmatter={blogs[index].frontmatter}
              url={blogs[index].url}
            />
          ))
        }
      </div>
    </div>
  </section>

  <!-- About section -->
  <section>
    <div
      class="px-4 py-12 mx-auto max-w-7xl sm:px-6 md:px-12 lg:px-24 lg:py-24"
    >
      <div class="flex flex-wrap items-center mx-auto max-w-7xl">
        <div class="w-full lg:max-w-lg lg:w-1/2 rounded-xl">
          <div>
            <div class="relative w-full max-w-lg">
              <div
                class="absolute top-0 rounded-full bg-violet-300 -left-4 w-72 h-72 mix-blend-multiply filter blur-xl opacity-70 animate-blob"
              >
              </div>

              <div
                class="absolute rounded-full bg-fuchsia-300 -bottom-24 right-20 w-72 h-72 mix-blend-multiply filter blur-xl opacity-70 animate-blob animation-delay-4000"
              >
              </div>
              <div class="relative">
                <img
                  class="object-cover object-center mx-auto rounded-lg shadow-2xl"
                  alt="hero"
                  loading="lazy"
                  src="https://res.cloudinary.com/aianshu/image/upload/f_auto,c_limit,w_384,q_auto/aianshume.jpeg"
                />
              </div>
            </div>
          </div>
        </div>
        <div
          class="flex flex-col items-start mt-12 mb-16 text-left lg:flex-grow lg:w-1/2 lg:pl-6 xl:pl-24 md:mb-0 xl:mt-0"
        >
          <span
            class="mb-8 text-xs font-bold tracking-widest text-blue-600 uppercase"
          >
            About Me
          </span>
          <h1
            class="mb-8 text-4xl font-heading leading-none tracking-tighter text-black md:text-7xl lg:text-5xl"
          >
            My passion is to build programs.
          </h1>
          <p class="mb-8 text-base leading-relaxed text-left text-gray-500">
            I'm a self-taught open source computer programmer 🧑‍💻 and internet
            entrepreneur 🤵, i can change a mac and wireless to miracle 💻🚀,
            madly love in with Science 🧑‍🔬
          </p>
